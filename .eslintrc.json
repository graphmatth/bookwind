{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:@tanstack/query/recommended"
  ],
  "plugins": [
    "@tanstack/query"
  ],
  "rules": {
    "prettier/prettier": ["error", { "trailingComma": "all" }],
    "no-unused-vars": "warn",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
